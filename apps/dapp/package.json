{
  "name": "dapp",
  "private": true,
  "version": "1.0.21",
  "type": "module",
  "scripts": {
    "dev": "vite --port ${VITE_APP_PORT-5173}",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:visual-tests": "source .env && chromatic --project-token=$STORYBOOK_CHROMATIC_PROJECT_TOKEN",
    "generate:openapi": "pnpm --filter=@repo/cloak run generate:openapi",
    "env:testnet-mock": "dotenvx run -f .env.testnet-mock",
    "env:mainnet-mock": "dotenvx run -f .env.mainnet-mock",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome --spec 'cypress/smoke.cy.ts'",
    "cy:smoke": "start-server-and-test dev ${VITE_APP_URL} \"pnpm cy:run\"",
    "test:smoke:mainnet": "pnpm env:mainnet-mock -- pnpm cy:smoke",
    "test:smoke:testnet": "pnpm env:testnet-mock -- pnpm cy:smoke",
    "test:unit": "vitest run",
    "test": "pnpm run test:unit && pnpm /^test:smoke:/"
  },
  "dependencies": {
    "@axis-finance/sdk": "0.0.4",
    "@fleekhq/fleek-storage-js": "^1.0.24",
    "@openzeppelin/merkle-tree": "^1.0.7",
    "@rainbow-me/rainbowkit": "2.0.2",
    "@repo/abis": "workspace:*",
    "@repo/cloak": "workspace:*",
    "@repo/deployments": "workspace:*",
    "@repo/env": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/ipfs-api": "workspace:*",
    "@repo/points": "workspace:*",
    "@repo/subgraph-client": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tanstack/react-query": "5.18.1",
    "@tanstack/react-query-devtools": "^5.62.2",
    "@tanstack/react-table": "^8.20.5",
    "@trpc/client": "^10.45.2",
    "@uniswap/v3-sdk": "^3.19.0",
    "@xyfinance/widget": "^4.9.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "date-fns": "^3.6.0",
    "debounce": "^2.2.0",
    "ethereum-blockies": "^0.1.1",
    "express": "^4.21.1",
    "jotai": "^2.10.3",
    "lucide-react": "^0.439.0",
    "papaparse": "^5.4.1",
    "plausible-tracker": "^0.3.9",
    "react": "18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "18.3.1",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.28.0",
    "react-scroll": "^1.9.0",
    "react-use-wizard": "^2.3.0",
    "recharts": "^2.14.1",
    "viem": "2.21.54",
    "vite-plugin-node-polyfills": "^0.19.0",
    "vite-tsconfig-paths": "^4.3.2",
    "wagmi": "2.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@dotenvx/dotenvx": "^1.28.0",
    "@hookform/devtools": "^4.3.1",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/addon-links": "^8.4.6",
    "@storybook/blocks": "^8.4.6",
    "@storybook/react": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/test": "^8.4.6",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@types/ethereum-blockies": "^0.1.2",
    "@types/node": "^20.17.9",
    "@types/papaparse": "^5.3.15",
    "@types/react": "18.2.48",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "18.3.1",
    "@types/react-scroll": "^1.8.10",
    "@vitejs/plugin-react": "^4.3.4",
    "angry-purple-tiger": "^1.0.5",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.16.1",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.8.0",
    "msw": "^2.6.6",
    "postcss": "^8.4.49",
    "start-server-and-test": "^2.0.8",
    "storybook": "^8.4.6",
    "storybook-addon-remix-react-router": "^3.0.2",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vitest": "^1.6.0"
  }
}
