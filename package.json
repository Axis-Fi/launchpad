{
  "name": "axis-frontend",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "postbuild": "turbo run lint",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,css,html,yaml}\"",
    "prepare": "husky",
    "storybook": "turbo run storybook"
  },
  "devDependencies": {
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "turbo": "^1.12.4"
  },
  "packageManager": "pnpm@8.9.0",
  "resolutions": {
    "graphql": "16.8.1"
  }
}
